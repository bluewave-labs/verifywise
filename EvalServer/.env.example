# EvalServer Environment Variables
#
# To get started, copy this file to .env:
#   cp .env.example .env
#
# Then update the values as needed for your environment.

# Database Configuration (required)
DB_USER=postgres
DB_PASSWORD=test
DB_HOST=localhost
DB_PORT=5432
DB_NAME=verifywise

# Backend URL for CORS (optional, defaults to http://localhost:3000)
BACKEND_URL=http://localhost:3000

# Redis URL (optional, defaults to redis://localhost:6379/0)
REDIS_URL=redis://localhost:6379/0

# Server Port (optional, defaults to 8000)
LLM_EVALS_PORT=8000
